[constants]

[built-in options]
c_std = 'c17'
cpp_std = 'c++20'
optimization = '3'
cpp_args = '-std=c++23'

[binaries]
c = '/usr/bin/gcc'
cpp = '/opt/homebrew/Cellar/gcc@11/11.4.0/bin/g++-11'

[cmake]
